<div style="float: right">
  <%= link_to 'Back to session', :action=>'show', :id=>@sess %> | 
  <%= link_to 'Back to project', :controller=>'projects', :action => 'show', :id=>@sess.project %>
</div>
<h1>Editing <%= @sess.project.name %> session</h1>

<% form_tag :action => 'update', :id => @sess do %>
  <%= render :partial => 'form' %>
  <%= submit_tag 'Save' %>
<% end %>

